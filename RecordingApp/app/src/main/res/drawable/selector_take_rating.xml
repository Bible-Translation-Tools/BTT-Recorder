<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/wycliffeassociates.recordingapp"
    >

    <item
        app:state_light="false"
        app:state_zero="true"
        android:drawable="@drawable/star_o"
        />

    <item
        app:state_one="true"
        android:drawable="@drawable/star_red"
        />

    <item
        app:state_two="true"
        android:drawable="@drawable/star_yellow"
        />

    <item
        app:state_three="true"
        android:drawable="@drawable/star_green"
        />

    <!--<item android:drawable="@drawable/star_o" />-->

    <item
        app:state_light="true"
        app:state_zero="true"
        android:drawable="@drawable/star_white_o"
        />

    <item
        app:state_light="true"
        android:drawable="@drawable/star_white_o" />

</selector>

{
    "resource": "bible_resources",
    "anthology": {
        "slug": "tq",
        "name": "translation questions",
        "file_conv": "1011111101",
        "parser": {
            "regex": "([a-zA-Z]{2,3}[-[\\d\\w]+]*)_tq_([a-zA-Z]{3})_b([\\d]{2})_(tq-[1-3]*[a-zA-Z]+)_c([\\d]{2,3})_v([\\d]{2,3})(-([\\d]{2,3}))?(_t([\\d]{2}))?(.wav)?",
            "groups": {
                "language": 1,
                "version": 2,
                "book_number": 3,
                "book": 4,
                "chapter": 5,
                "start_verse": 6,
                "end_verse": 7,
                "take": 10
            }
        }
    },
    "modes": [
        {
            "name":"question",
            "type":"single"
        }
    ],
    "books": "bible_books.json",
    "chunks": "bibleChunks/",
    "versions": "bibleVersions.json",
    "chunk_plugin": {
        "jar" : "questionschunk.jar",
        "class": "org.wycliffeassociates.translationrecorder.questionschunk.QuestionsChunkPlugin"
    }
}
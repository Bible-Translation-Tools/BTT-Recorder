{
    "resource": "bible_resources",
    "anthology": {
        "slug": "tn",
        "name": "translation notes",
        "file_conv": "1011110101",
        "parser": {
            "regex": "([a-zA-Z]{2,3}[-[\\d\\w]+]*)_tn_([\\w\\d]*)_b([\\d]*)_([\\w\\d]*-ch-[\\d]*)_v([\\d]{2,3})(-([\\d]{2,3}))?(_t([\\d]{2}))?(.wav)?",
            "groups": {
                "language": 1,
                "version": 2,
                "book_number": 3,
                "book": 4,
                "start_verse": 5,
                "end_verse": 6,
                "take": 9
            }
        }
    },
    "modes": [
        {
            "name":"note",
            "type":"single"
        }
    ],
    "books": "note_books.json",
    "chunks": "bibleChunks/",
    "versions": "bibleVersions.json",
    "chunk_plugin": {
        "jar" : "noteschunk.jar",
        "class": "org.wycliffeassociates.translationrecorder.noteschunk.NotesChunkPlugin"
    }
}